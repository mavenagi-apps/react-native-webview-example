# Maven Chat Configuration Template
# Copy this file to .env and fill in your values
# Run: node setup-maven-keys.js

# Your Maven organization and agent IDs (required)
MAVEN_ORG_ID=
MAVEN_AGENT_ID=

# Cryptographic keys (generated by setup-maven-keys.js --force)
MAVEN_PRIVATE_KEY=
MAVEN_PUBLIC_KEY=
MAVEN_ENCRYPTION_SECRET=

# Test user information (required for JWT generation)
TEST_USER_ID=
TEST_USER_FIRST_NAME=
TEST_USER_LAST_NAME=
TEST_USER_EMAIL=

# Generated JWT token (auto-generated by setup-maven-keys.js)
MAVEN_JWT_TOKEN=
